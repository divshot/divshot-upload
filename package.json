{
  "name": "divshot-upload",
  "version": "0.1.20",
  "description": "Upload app files to Divshot",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-server && npm run test-browser",
    "test-server": "node_modules/.bin/mocha --reporter spec test/server/**/*.js",
    "test-browser": "node_modules/.bin/testling | node_modules/.bin/tspec",
    "build": "npm run bundle-directive",
    "bundle": "node_modules/.bin/browserify test/browser/app.js -s upload > test/browser/bundle.js",
    "bundle-debug": "node_modules/.bin/browserify test/browser/app.js -s upload -d > test/browser/bundle.js",
    "bundle-directive": "browserify lib/browser/angular-directive.js > dist/upload.directive.js",
    "bundle-directive-test": "node_modules/.bin/browserify lib/browser/angular-directive.js -d > test/browser/upload.directive.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/divshot/divshot-upload.git"
  },
  "author": "Divshot",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/divshot/dio-upload/issues"
  },
  "devDependencies": {
    "browserify": "~4.1.2",
    "grunt": "~0.4.2",
    "uglifyjs": "~2.3.6",
    "watchify": "~0.5.4"
  },
  "testling": {
    "files": [
      "test/browser/**/*.js"
    ],
    "harness": "mocha",
    "browsers": [
      "iexplore/10.0",
      "iexplore/9.0",
      "firefox/16..latest",
      "chrome/22..latest",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  },
  "dependencies": {
    "drag-and-drop-files": "0.0.1",
    "JSONStream": "~0.7.1",
    "as-stream": "~0.1.1",
    "divshot-api": "0.6.x",
    "filereader-stream": "~0.1.1",
    "home-dir": "~0.1.2",
    "hyperquest": "~0.3.0",
    "is-node": "0.0.0",
    "readable-stream": "~1.1.10",
    "through": "~2.3.4"
  }
}
